{
  "expo": {
    "scheme": "myapp",
    "name": "app",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/fa.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/fa.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/fa.png",
        "backgroundColor": "#000000"
      },
      "package": "com.anonymous.app"
    },
    "web": {
      "favicon": "./assets/fa.png"
    },
    "plugins": [
      "expo-router",
      "expo-video",
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ]
    ]
  }
}
